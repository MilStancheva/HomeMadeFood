@model EditDailyMenuViewModel
@{
    ViewBag.Title = "AddDailyMenu";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    @Html.Partial("_AdminSideBarMenu")
    <div class="col-md-10">
        <h2>Add Daily Menu</h2>
        @Html.Action("AddMenu", Model)
</div>
<div>
    @Html.ActionLink("Back to List", "Index")
</div>

@section scripts {
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/adminindexpage")
    <script src="~/Scripts/Custom/daily-menu-accordion.js"></script>
    <script src="~/Scripts/Custom/datepicker-init.js"></script>
}